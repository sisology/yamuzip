<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="team.trillion.yamuzip.dobby.model.dao.DobbyRegistMapper">

    <insert id="registDobby">
        INSERT INTO tbl_dob_apply(user_code, apply_career)
        VALUES (#{userCode}, #{applyCareer})
    </insert>

    <select id="selectDobbyApplyByUserCode">
        SELECT COUNT(*)
        FROM tbl_dob_apply
        WHERE user_code = #{userCode}
    </select>
</mapper>